{
	"browserslist": "> 0.75%",
	"devDependencies": {
		"@auth/core": "0.18.6",
		"@auth/d1-adapter": "0.2.9",
		"@builder.io/qwik": "1.3.1",
		"@builder.io/qwik-auth": "0.1.3",
		"@builder.io/qwik-city": "1.3.1",
		"@cloudflare/workers-types": "4.20231218.0",
		"@fortawesome/fontawesome-common-types": "6.5.1",
		"@fortawesome/free-brands-svg-icons": "6.5.1",
		"@fortawesome/pro-duotone-svg-icons": "6.5.1",
		"@fortawesome/pro-regular-svg-icons": "6.5.1",
		"@miniflare/d1": "2.14.1",
		"@miniflare/shared": "2.14.1",
		"@toast-ui/editor": "3.2.2",
		"@types/eslint": "8.44.9",
		"@types/node": "20.10.5",
		"@typescript-eslint/eslint-plugin": "6.15.0",
		"@typescript-eslint/parser": "6.15.0",
		"@unocss/eslint-config": "0.58.0",
		"@unocss/postcss": "0.58.0",
		"@unocss/reset": "0.58.0",
		"@unpic/qwik": "0.0.38",
		"@vitest/ui": "1.1.0",
		"better-sqlite3": "9.2.2",
		"eslint": "8.56.0",
		"eslint-plugin-qwik": "1.3.1",
		"postcss-dropunusedvars": "2.2.1",
		"postcss-lightningcss": "1.0.0",
		"postcss-variable-compress": "3.0.0",
		"prettier": "3.1.1",
		"sass": "1.69.5",
		"typescript": "5.3.3",
		"undici": "6.0.1",
		"unocss": "0.58.0",
		"vite": "5.0.10",
		"vite-tsconfig-paths": "4.2.2",
		"vitest": "1.1.0",
		"wrangler": "3.21.0"
	},
	"engines": {
		"node": "^20.10.0",
		"pnpm": "^8.11.0"
	},
	"packageManager": "pnpm@8.11.0",
	"private": true,
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"d1-setup": "wrangler d1 execute localdb --local --file=./scripts/d1-setup.sql",
		"deploy": "wrangler pages publish ./dist",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"preview": "qwik build preview && vite preview --open",
		"qwik": "qwik",
		"serve": "wrangler pages dev ./dist",
		"start": "vite --open --mode ssr",
		"test.unit": "vitest components",
		"test.unit.ui": "vitest --ui components"
	},
	"trustedDependencies": [
		"sharp"
	]
}
