{
	"browserslist": "> 0.75%",
	"devDependencies": {
		"@auth/core": "0.16.1",
		"@builder.io/qwik": "1.2.13",
		"@builder.io/qwik-auth": "0.1.3",
		"@builder.io/qwik-city": "1.2.13",
		"@cloudflare/workers-types": "4.20231002.0",
		"@fortawesome/fontawesome-common-types": "6.4.2",
		"@fortawesome/free-brands-svg-icons": "6.4.2",
		"@fortawesome/pro-duotone-svg-icons": "6.4.2",
		"@fortawesome/pro-regular-svg-icons": "6.4.2",
		"@miniflare/d1": "2.14.1",
		"@miniflare/shared": "2.14.1",
		"@types/eslint": "8.44.3",
		"@types/node": "20.8.4",
		"@typescript-eslint/eslint-plugin": "6.7.5",
		"@typescript-eslint/parser": "6.7.5",
		"@unocss/eslint-config": "0.56.5",
		"@unocss/postcss": "0.56.5",
		"@unocss/reset": "0.56.5",
		"@vitest/ui": "0.34.6",
		"better-sqlite3": "8.7.0",
		"eslint": "8.51.0",
		"eslint-plugin-qwik": "1.2.13",
		"postcss-dropunusedvars": "2.2.1",
		"postcss-lightningcss": "1.0.0",
		"postcss-variable-compress": "3.0.0",
		"prettier": "3.0.3",
		"sass": "1.69.0",
		"typescript": "5.2.2",
		"undici": "5.25.4",
		"unocss": "0.56.5",
		"vite": "4.4.11",
		"vite-tsconfig-paths": "4.2.1",
		"vitest": "0.34.6",
		"wrangler": "3.11.0"
	},
	"engines": {
		"node": "20.8.0",
		"pnpm": "8.9.0"
	},
	"packageManager": "pnpm@8.9.0",
	"private": true,
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"d1-setup": "wrangler d1 execute localdb --local --file=./scripts/d1-setup.sql",
		"deploy": "wrangler pages publish ./dist",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"preview": "qwik build preview && vite preview --open",
		"qwik": "qwik",
		"serve": "wrangler pages dev ./dist",
		"start": "vite --open --mode ssr",
		"test.unit": "vitest components",
		"test.unit.ui": "vitest --ui components"
	},
	"trustedDependencies": [
		"sharp"
	]
}
